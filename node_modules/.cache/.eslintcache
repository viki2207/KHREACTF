mport { Provider } from \"react-redux\";\nimport store from \"./redux/store\";\nimport { useEffect } from \"react\";\nimport setAuthToken from \"./utils/setAuthToken\";\nimport { loadUser } from \"./redux/actions/authAction\";\n\nfunction App() {\n  const navigate = useNavigate();\n  useEffect(() => {\n    console.log(localStorage.token);\n    if (localStorage.token) {\n      setAuthToken(localStorage.token);\n    } else {\n      navigate(\"/\");\n      // send the user to landing page.\n    }\n    store.dispatch(loadUser());\n  }, []); // we are not accessing any props.\n  const appName = \"UpgradeConnector\";\n  return (\n    <>\n      <Provider store={store}>\n        <Header appName={appName}></Header>\n\n        <Routers></Routers>\n        <Footer appName={appName}></Footer>\n      </Provider>\n    </>\n  );\n}\n\nexport default App;\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/reportWebVitals.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/utils/setAuthToken.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/actions/authAction.js",["214"],[],"// registration\n// login\n// loading current user\nimport axios from \"axios\";\nimport { REGISTER_SUCCESS, USER_LOADED, USER_ERROR, LOGOUT } from \"../types\";\nimport { setAlert } from \"./alertAction\";\nimport api from \"../../utils/api\";\n\nexport const logout = () => ({ type: LOGOUT });\nexport const loadUser = () => async (dispatch) => {\n  try {\n    const res = await api.get(\"/auth\"); // we have to provide the token ?\n    dispatch({ type: USER_LOADED, payload: res.data });\n  } catch (err) {\n    console.log(err);\n    dispatch({ type: USER_ERROR });\n  }\n};\nexport const register =\n  ({ name, email, password }) =>\n  async (dispatch) => {\n    // used to register the user via performing the rest call.\n\n    const config = {\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n    };\n    const data = JSON.stringify({ name, email, password });\n    try {\n      console.log(data);\n      const res = await api.post(\"/users\", data, config);\n      dispatch({ type: REGISTER_SUCCESS, payload: res.data });\n      loadUser();\n    } catch (err) {\n      const errors = err.response.data.errors;\n      if (errors) {\n        errors.forEach((error) => dispatch(setAlert(error.msg, \"danger\")));\n      }\n      // lets traverse the errors array\n      // to process the error i.e. display work should be handled by an action .\n      /// let have alertAction.\n    }\n  };\n// export : we can use this function in any file\n// const : keyword\n// register : name of the function\n//({name,email,password}) : json object which is accepted by a function/ method\n// as an arguement caller===> component.\n// async : asynchronous operations\n// dispatch :\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/routers/Routers.js",["215"],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/layouts/Header.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/layouts/Footer.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/layouts/Landing.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/store/index.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/utils/api.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/actions/alertAction.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/layouts/Alert.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/routers/PrivateRoute.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/auth/Register.jsx",["216","217"],[],"import React, { Component } from \"react\";\nimport axios from \"axios\";\nimport classnames from \"classnames\";\nexport default class Register extends Component {\n  constructor() {\n    super(); // super method : it is telling us to call the base class constructor\n    console.log(\"hello from register\");\n    this.state = {\n      name: \"Abhi\",\n      email: \"\",\n      password: \"\",\n      confirmPassword: \"\",\n      errors: {},\n    };\n  }\n  // e : event which will help us to get the data.\n  onChange = (e) => {\n    this.setState({ [e.target.name]: e.target.value }); // this function is responsible for modifying the state.\n  };\n  // to handle the change event for input tags\n  onSubmit = (e) => {\n    e.preventDefault();\n    console.log(JSON.stringify(this.state));\n    axios\n      .post(\"/api/users\", this.state)\n      .then((res) => console.log(JSON.stringify(res)))\n      .catch((err) => {\n        const errorObj = {};\n        console.error(JSON.stringify(err.response.data.errors));\n        err.response.data.errors.forEach((element) => {\n          console.log({ msg: element.msg, field: element.param });\n          if (element.param == \"email\") errorObj.email = element.msg;\n          if (element.param == \"password\") errorObj.password = element.msg;\n        });\n        this.setState({ errors: errorObj });\n      });\n    console.log(\"hello from register form\");\n  };\n  /// to handle submition of form.\n\n  render() {\n    const { name, email, password, confirmPassword } = this.state;\n    return (\n      <>\n        <section class=\"container\">\n          <h1 class=\"large text-primary\">Sign Up</h1>\n          <p class=\"lead\">\n            <i class=\"fas fa-user\"></i> Create Your Account\n          </p>\n          <form class=\"form\" onSubmit={this.onSubmit}>\n            <div class=\"form-group\">\n              <input\n                type=\"text\"\n                placeholder=\"Name\"\n                name=\"name\"\n                value={name}\n                required\n                onChange={this.onChange}\n              />\n            </div>\n            <div class=\"form-group\">\n              <input\n                type=\"email\"\n                placeholder=\"Email Address\"\n                name=\"email\"\n                value={email}\n                className={classnames(\"form- control\", {\n                  \"is-invalid\": this.state.errors.email,\n                })}\n                onChange={this.onChange}\n              />\n              <div className=\"d-block invalid-feedback\">\n                {this.state.errors.email}\n              </div>\n              <small class=\"form-text\">\n                This site uses Gravatar so if you want a profile image, use a\n                Gravatar email\n              </small>\n            </div>\n            <div class=\"form-group\">\n              <input\n                type=\"password\"\n                placeholder=\"Password\"\n                name=\"password\"\n                minLength=\"6\"\n                value={password}\n                onChange={this.onChange}\n                className={classnames(\"form-control form-control-lg\", {\n                  \"is-invalid\": this.state.errors && this.state.errors.password,\n                })}\n              />\n              <div className=\"d-block invalid-feedback\">\n                {this.state.errors.password}\n              </div>\n            </div>\n            <div class=\"form-group\">\n              <input\n                type=\"password\"\n                placeholder=\"Confirm Password\"\n                name=\"password2\"\n                minLength=\"6\"\n                value={confirmPassword}\n                onChange={this.onChange}\n              />\n            </div>\n            <input type=\"submit\" class=\"btn btn-primary\" value=\"Register\" />\n          </form>\n          <p class=\"my-1\">\n            Already have an account? <a href=\"login.html\">Sign In</a>\n          </p>\n        </section>\n      </>\n    );\n  }\n}\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/auth/Login.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/auth/Register2.jsx",["218","219"],[],"import React, { useState } from \"react\";\nimport axios from \"axios\";\nimport classnames from \"classnames\";\nimport PropTypes from \"prop-types\";\n\nimport { connect } from \"react-redux\";\n\n// action import\nimport { register } from \"../../redux/actions/authAction\";\nimport { Navigate } from \"react-router\";\nconst initialState = {\n  name: \"Abhi\",\n  email: \"\",\n  password: \"\",\n  confirmPassword: \"\",\n};\n\n// rfcreduxp:\n// react function componet redux prop types.\n\nexport const Register2 = ({ isAuthenticated, register }) => {\n  // (props) : we provided all actions and state props delcaration.\n  const [formData, setFormData] = useState(initialState);\n\n  const { name, email, password, confirmPassword } = formData;\n  // destructuring of the state object.\n\n  const [error, setError] = useState({});\n  // to hold the error related messages from restthe rest api call.\n  const onChange = (e) => {\n    setFormData({ ...formData, [e.target.name]: e.target.value });\n    // ...formData : spread operator.\n\n    //this.setState({ [e.target.name]: e.target.value });\n  };\n\n  if (isAuthenticated) {\n    return <Navigate to=\"/dashboard\"></Navigate>;\n  }\n  const onSubmit = (e) => {\n    e.preventDefault();\n    if (password === confirmPassword) {\n      console.log(JSON.stringify(formData));\n\n      register({ name, email, password });\n    }\n  };\n  return (\n    <>\n      <section class=\"container\">\n        <h1 class=\"large text-primary\">Sign Up for </h1>\n        <p class=\"lead\">\n          <i class=\"fas fa-user\"></i> Create Your Account\n        </p>\n        <form class=\"form\" onSubmit={onSubmit}>\n          <div class=\"form-group\">\n            <input\n              type=\"text\"\n              placeholder=\"Name\"\n              name=\"name\"\n              value={name}\n              required\n              onChange={onChange}\n            />\n          </div>\n          <div class=\"form-group\">\n            <input\n              type=\"email\"\n              placeholder=\"Email Address\"\n              name=\"email\"\n              value={email}\n              className={classnames(\"form- control\", {\n                \"is-invalid\": error.email,\n              })}\n              onChange={onChange}\n            />\n            <div className=\"d-block invalid-feedback\">{error.email}</div>\n            <small class=\"form-text\">\n              This site uses Gravatar so if you want a profile image, use a\n              Gravatar email\n            </small>\n          </div>\n          <div class=\"form-group\">\n            <input\n              type=\"password\"\n              placeholder=\"Password\"\n              name=\"password\"\n              minLength=\"6\"\n              value={password}\n              onChange={onChange}\n              className={classnames(\"form-control form-control-lg\", {\n                \"is-invalid\": error && error.password,\n              })}\n            />\n            <div className=\"d-block invalid-feedback\">{error.password}</div>\n          </div>\n          <div class=\"form-group\">\n            <input\n              type=\"password\"\n              placeholder=\"Confirm Password\"\n              name=\"confirmPassword\"\n              minLength=\"6\"\n              value={confirmPassword}\n              onChange={onChange}\n            />\n            <div className=\"d-block invalid-feedback\">\n              {error.confirmPassword}\n            </div>\n          </div>\n          <input type=\"submit\" class=\"btn btn-primary\" value=\"Register\" />\n        </form>\n        <p class=\"my-1\">\n          Already have an account? <a href=\"login.html\">Sign In</a>\n        </p>\n      </section>\n    </>\n  );\n};\n\nRegister2.propTypes = {\n  register: PropTypes.func.isRequired,\n  isAuthenticated: PropTypes.bool,\n};\n// PropTypes : we will define the types of our used props in our component\n\nconst mapStateToProps = (state) => ({\n  // state : global state / app state / store\n  isAuthenticated: state.authReducer.isAuthenticated,\n});\n// map your props with store content\n\nconst mapDispatchToProps = {\n  register,\n};\n// we will list out all the actions consumed by the component.\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Register2);\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/dashboard/Dashboard.jsx",["220","221"],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/Profile.jsx",["222"],[],"import PropTypes from \"prop-types\";\nimport React, { useEffect } from \"react\";\nimport { connect } from \"react-redux\";\nimport { useParams } from \"react-router\";\nimport { Link } from \"react-router-dom\";\nimport { getProfileById } from \"../../redux/actions/profileAction\";\nimport Spinner from \"../layouts/Spinner\";\nimport ProfileAbout from \"./ProfileAbout\";\nimport ProfileEducation from \"./ProfileEducation\";\nimport ProfileExperience from \"./ProfileExperience\";\nimport ProfileGitHub from \"./ProfileGitHub\";\nimport ProfileTop from \"./ProfileTop\";\nexport const Profile = ({ getProfileById, profile: { profile }, auth }) => {\n  const { id } = useParams();\n  // we may collect one or more params from the routing path as per hte req.\n  // it will help us to get the userId from the routing path .i.e. it help us to get the :id value.\n  useEffect(() => {\n    getProfileById(id);\n  }, [getProfileById, id]);\n  const editProfileButton = (\n    <Link to=\"/edit-profile\" className=\"btn btn-dark\">\n      Edit Profile\n    </Link>\n  );\n  const profileGithubUserName = \"\";\n  // (\n  //   <ProfileGitHub username={profile.githubusername} />\n  // );\n\n  return (\n    <div>\n      {\" \"}\n      <section className=\"container\">\n        {profile === null ? (\n          <Spinner />\n        ) : (\n          <>\n            <Link to=\"/profiles\" className=\"btn btn-light\">\n              Back To Profiles\n            </Link>\n            {auth.isAuthenticated &&\n              auth.loading === false &&\n              auth.user._id === profile.user._id &&\n              editProfileButton}\n            <div className=\"profile-grid my-1\">\n              <ProfileTop profile={profile} />\n              <ProfileAbout profile={profile} />\n              <div className=\"profile-exp bg-white p-2\">\n                <h2 className=\"text-primary\">Experience</h2>\n                {profile.experience.length > 0 ? (\n                  <>\n                    {profile.experience.map((e) => (\n                      <ProfileExperience key={e._id} experience={e} />\n                    ))}\n                  </>\n                ) : (\n                  <h4>No experience credentials</h4>\n                )}\n              </div>\n\n              <div className=\"profile-edu bg-white p-2\">\n                <h2 className=\"text-primary\">Education</h2>\n                {profile.education.length > 0 ? (\n                  <>\n                    {profile.education.map((e) => (\n                      <ProfileEducation key={e._id} education={e} />\n                    ))}\n                  </>\n                ) : (\n                  <h4>No education credentials</h4>\n                )}\n              </div>\n\n              {profile.githubusername && profileGithubUserName}\n            </div>\n          </>\n        )}\n      </section>\n    </div>\n  );\n};\n\nProfile.propTypes = {\n  getProfileById: PropTypes.func.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  profile: state.profileReducer,\n  auth: state.authReducer,\n});\n\nconst mapDispatchToProps = { getProfileById };\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Profile);\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profiles/Profiles.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/CreateProfile.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/types/index.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/reducers/index.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/layouts/Spinner.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/actions/profileAction.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/dashboard/DashboardAction.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/ProfileAbout.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/ProfileEducation.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/ProfileTop.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profiles/ProfileItem.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/ProfileExperience.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile/ProfileGitHub.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/reducers/authReducer.js",["223","224"],[],"import { USER_LOADED, REGISTER_SUCCESS, REGISTER_FAIL, LOGOUT } from \"../types\";\n// it is used to hold and manipulate the user related states in store.\n// all auth related operations / user related operaations like  register , login  , loading current user info. etc.\nconst initialState = {\n  token: null,\n  isAuthenticated: null,\n  loading: true,\n  user: null,\n};\n// it will be part of ur global state / store where we can hold all user related data.\n\nexport default (state = initialState, action) => {\n  // action will share the data needs to be manipulated and it will share the flag\n  // data : payload\n  const { type, payload } = action;\n  switch (type) {\n    case LOGOUT:\n      localStorage.removeItem(\"token\");\n      return {\n        token: null,\n        isAuthenticated: null,\n        loading: true,\n        user: null,\n      };\n    case USER_LOADED:\n      // to load the user info\n      return { ...state, isAuthenticated: true, loading: false, user: payload };\n    case REGISTER_SUCCESS:\n      // to handle the success part of user registration\n      // it will give us the token.\n      // isAuthenticated : true\n      /// loading : false\n      // token : we need to use the token from dashboard on wards\n      // for create profile / add exp add education etc.\n      localStorage.setItem(\"token\", payload.token);\n      // localStorage will help us to hold the token in persited way.\n\n      return { ...state, ...payload, isAuthenticated: true, loading: false };\n\n    default:\n      return state;\n  }\n};\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/reducers/alertReducer.js",["225"],[],"import { REMOVE_ALERT, SET_ALERT } from \"../types\";\n\nconst initialState = [];\nexport default (state = initialState, action) => {\n  const { type, payload } = action;\n  switch (type) {\n    case SET_ALERT:\n      console.log(JSON.stringify(payload));\n      // it will brings the array.\n      return [...state, payload];\n\n    case REMOVE_ALERT:\n      // payload brings the id.\n\n      return state.filter((alert) => alert.id !== payload);\n    default:\n      return state;\n  }\n};\n","/Users/vidhi/Desktop/fullstack/KHREACTF/src/redux/reducers/profileReducer.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/utils/formatDate.js",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile-forms/AddExperience.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/dashboard/DisplayExpDetails.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/dashboard/DisplayEduDetails.jsx",[],[],"/Users/vidhi/Desktop/fullstack/KHREACTF/src/components/profile-forms/AddEducation.jsx",[],[],{"ruleId":"226","severity":1,"message":"227","line":1,"column":8,"nodeType":"228","messageId":"229","endLine":1,"endColumn":12},{"ruleId":"226","severity":1,"message":"230","line":4,"column":8,"nodeType":"228","messageId":"229","endLine":4,"endColumn":15},{"ruleId":"226","severity":1,"message":"231","line":7,"column":27,"nodeType":"228","messageId":"229","endLine":7,"endColumn":33},{"ruleId":"232","severity":1,"message":"233","line":27,"column":6,"nodeType":"234","end